<Window x:Class="SnesBonus.Views.ResultPicker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ResultPicker" Height="455" Width="286">
    <ListView x:Name="ListView" MouseDoubleClick="ListView_OnMouseDoubleClick">
        <ListView.View>
            <GridView>
                <GridViewColumn Width="270" DisplayMemberBinding="{Binding Title}" Header="Name" />
            </GridView>
        </ListView.View>
    </ListView>
</Window>
